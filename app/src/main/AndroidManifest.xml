        <activity android:name=".layout.VIewProductActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".layout.TabActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".layout.UserSettingActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".layout.ModifyUserInfoActivity"
